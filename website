Â  Â  Â  Â  }




Â  Â  Â  Â  function createNoMatchCard(track) {
Â  Â  Â  Â  Â  Â  const trackName = typeof track === 'string' ? track : (track.name || 'Unknown Track');
Â  Â  Â  Â  Â  Â  const artistName = typeof track === 'string' ? '' : (track.artist || '');
Â  Â  Â  Â  Â  Â Â 
