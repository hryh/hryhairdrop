Â  Â  Â  Â  function processMappingResults(content) {
Â  Â  Â  Â  Â  Â  try {
Â  Â  Â  Â  Â  Â  Â  Â  // Try to extract JSON from the response
Â  Â  Â  Â  Â  Â  Â  Â  let jsonStr = content;
Â  Â  Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  Â  Â  // Remove markdown code blocks if present
Â  Â  Â  Â  Â  Â  Â  Â  jsonStr = jsonStr.replace(/```json\n?/g, '').replace(/```\n?/g, '');
Â  Â  Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  Â  Â  // Try to find JSON within the text
Â  Â  Â  Â  Â  Â  Â  Â  const jsonMatch = jsonStr.match(/\{[\s\S]*\}/);
Â  Â  Â  Â  Â  Â  Â  Â  if (jsonMatch) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  jsonStr = jsonMatch[0];
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  Â  Â  const results = JSON.parse(jsonStr);
Â  Â  Â  Â  Â  Â  Â  Â  mappingResults = results;
Â  Â  Â  Â  Â  Â  Â  Â  displayResults(results);
Â  Â  Â  Â  Â  Â  } catch (error) {
Â  Â  Â  Â  Â  Â  Â  Â  console.error('Error parsing results:', error);
Â  Â  Â  Â  Â  Â  Â  Â  // Fallback: display raw content as text
Â  Â  Â  Â  Â  Â  Â  Â  displayTextResults(content);
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }




Â  Â  Â  Â  function displayResults(results) {
