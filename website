Â  Â  Â  Â  function formatDuration(duration) {
Â  Â  Â  Â  Â  Â  if (!duration) return '';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  let ms = parseInt(duration);
Â  Â  Â  Â  Â  Â  if (isNaN(ms)) return duration;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const seconds = Math.floor(ms / 1000);
Â  Â  Â  Â  Â  Â  const minutes = Math.floor(seconds / 60);
Â  Â  Â  Â  Â  Â  const remainingSeconds = seconds % 60;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return `${minutes}:${remainingSeconds.toString().padStart(2, '0')}`;
Â  Â  Â  Â  }




