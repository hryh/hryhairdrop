Â  Â  Â  Â  Â  Â  const resultsList = document.getElementById('results-list');
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  resultsSection.classList.remove('hidden');




Â  Â  Â  Â  Â  Â  if (!results.success) {
Â  Â  Â  Â  Â  Â  Â  Â  resultsSummary.innerHTML = `
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="bg-red-50 dark:bg-red-900/20 border border-red-200 dark:border-red-800 rounded-lg p-4">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="flex">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-red-600 dark:text-red-400">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  âŒ No matches found: ${results.error || 'Unknown error'}
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  Â  Â  `;
Â  Â  Â  Â  Â  Â  Â  Â  resultsList.innerHTML = '';
Â  Â  Â  Â  Â  Â  Â  Â  return;
Â  Â  Â  Â  Â  Â  }




Â  Â  Â  Â  Â  Â  // Display summary
Â  Â  Â  Â  Â  Â  const totalTracks = results.totalTracks || (results.results ? results.results.length : (results.matches ? 1 : 0));
Â  Â  Â  Â  Â  Â  const mappedTracks = results.mappedTracks || (results.matches ? 1 : (results.results ? results.results.filter(r => r.appleMusic).length : 0));
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  resultsSummary.innerHTML = `
Â  Â  Â  Â  Â  Â  Â  Â  <div class="grid grid-cols-1 md:grid-cols-3 gap-4">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="bg-green-50 dark:bg-green-900/20 rounded-lg p-4 text-center">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-2xl font-bold text-green-600 dark:text-green-400">${totalTracks}</div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-sm text-green-800 dark:text-green-200">Total Tracks</div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="bg-blue-50 dark:bg-blue-900/20 rounded-lg p-4 text-center">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-2xl font-bold text-blue-600 dark:text-blue-400">${mappedTracks}</div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-sm text-blue-800 dark:text-blue-200">Successful Matches</div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="bg-purple-50 dark:bg-purple-900/20 rounded-lg p-4 text-center">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-2xl font-bold text-purple-600 dark:text-purple-400">${Math.round((mappedTracks / totalTracks) * 100)}%</div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="text-sm text-purple-800 dark:text-purple-200">Match Rate</div>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  Â  Â  `;




Â  Â  Â  Â  Â  Â  // Display individual results
Â  Â  Â  Â  Â  Â  let resultsHtml = '';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  if (results.matches) {
Â  Â  Â  Â  Â  Â  Â  Â  // Single track result
Â  Â  Â  Â  Â  Â  Â  Â  results.matches.forEach(match => {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  resultsHtml += createResultCard(results.query, match);
Â  Â  Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  Â  Â  } else if (results.results) {
Â  Â  Â  Â  Â  Â  Â  Â  // Multiple tracks result
Â  Â  Â  Â  Â  Â  Â  Â  results.results.forEach(result => {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  if (result.appleMusic) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  resultsHtml += createResultCard(result.spotify || result.parsed, result.appleMusic);
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  resultsHtml += createNoMatchCard(result.spotify || result.parsed || result.input);
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  resultsList.innerHTML = resultsHtml;
Â  Â  Â  Â  }




Â  Â  Â  Â  function createResultCard(spotifyTrack, appleMatch) {
Â  Â  Â  Â  Â  Â  const confidence = appleMatch.confidence || 85;
Â  Â  Â  Â  Â  Â  const confidenceColor = confidence >= 90 ? 'green' : confidence >= 70 ? 'yellow' : 'red';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return `
Â  Â  Â  Â  Â  Â  Â  Â  <div class="card-hover bg-white dark:bg-gray-800 border border-gray-200 dark:border-gray-700 rounded-lg p-6">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="flex items-start justify-between mb-4">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <div class="flex-1">
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <h3 class="font-semibold text-lg">${spotifyTrack.name}</h3>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  <p class="text-gray-600 dark:text-gray-400">${spotifyTrack.artist}</p>
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  ${spotifyTrack.album ? `<p class="text-sm text-gray-500 dark:text-gray-500">${spotifyTrack.album}</p>` : ''}
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  </div>
