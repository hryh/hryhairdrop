Â  Â  Â  Â  // Advanced matching functions
Â  Â  Â  Â  function calculateMatchConfidence(spotifyTrack, appleMusicTrack) {
Â  Â  Â  Â  Â  Â  let confidence = 0;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // ISRC match (highest priority)
Â  Â  Â  Â  Â  Â  if (spotifyTrack.isrc && appleMusicTrack.isrc && spotifyTrack.isrc === appleMusicTrack.isrc) {
Â  Â  Â  Â  Â  Â  Â  Â  return 100;
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Track name similarity
Â  Â  Â  Â  Â  Â  const nameMatch = calculateStringSimilarity(spotifyTrack.name.toLowerCase(), appleMusicTrack.name.toLowerCase());
Â  Â  Â  Â  Â  Â  confidence += nameMatch * 40;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Artist name similarity
Â  Â  Â  Â  Â  Â  const artistMatch = calculateStringSimilarity(spotifyTrack.artist.toLowerCase(), appleMusicTrack.artist.toLowerCase());
Â  Â  Â  Â  Â  Â  confidence += artistMatch * 35;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Album name similarity (if available)
Â  Â  Â  Â  Â  Â  if (spotifyTrack.album && appleMusicTrack.album) {
Â  Â  Â  Â  Â  Â  Â  Â  const albumMatch = calculateStringSimilarity(spotifyTrack.album.toLowerCase(), appleMusicTrack.album.toLowerCase());
Â  Â  Â  Â  Â  Â  Â  Â  confidence += albumMatch * 15;
Â  Â  Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  Â  Â  Â  confidence += 15; // Bonus for having one album
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Duration similarity (if available)
Â  Â  Â  Â  Â  Â  if (spotifyTrack.duration && appleMusicTrack.duration) {
Â  Â  Â  Â  Â  Â  Â  Â  const spotifyMs = parseDuration(spotifyTrack.duration);
Â  Â  Â  Â  Â  Â  Â  Â  const appleMs = parseDuration(appleMusicTrack.duration);
Â  Â  Â  Â  Â  Â  Â  Â  if (spotifyMs && appleMs) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  const durationDiff = Math.abs(spotifyMs - appleMs);
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  const durationMatch = Math.max(0, 1 - (durationDiff / 10000)); // 10 second tolerance
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  confidence += durationMatch * 10;
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return Math.round(Math.min(confidence, 100));
Â  Â  Â  Â  }




Â  Â  Â  Â  function calculateStringSimilarity(str1, str2) {
Â  Â  Â  Â  Â  Â  // Simple Levenshtein distance-based similarity
Â  Â  Â  Â  Â  Â  const maxLength = Math.max(str1.length, str2.length);
Â  Â  Â  Â  Â  Â  if (maxLength === 0) return 1;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const distance = levenshteinDistance(str1, str2);
Â  Â  Â  Â  Â  Â  return (maxLength - distance) / maxLength;
Â  Â  Â  Â  }




Â  Â  Â  Â  function levenshteinDistance(str1, str2) {
Â  Â  Â  Â  Â  Â  const matrix = [];
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  for (let i = 0; i <= str2.length; i++) {
Â  Â  Â  Â  Â  Â  Â  Â  matrix[i] = [i];
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  for (let j = 0; j <= str1.length; j++) {
Â  Â  Â  Â  Â  Â  Â  Â  matrix[0][j] = j;
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  for (let i = 1; i <= str2.length; i++) {
Â  Â  Â  Â  Â  Â  Â  Â  for (let j = 1; j <= str1.length; j++) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  if (str2.charAt(i - 1) === str1.charAt(j - 1)) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  matrix[i][j] = matrix[i - 1][j - 1];
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  matrix[i][j] = Math.min(
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  matrix[i - 1][j - 1] + 1,
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  matrix[i][j - 1] + 1,
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  matrix[i - 1][j] + 1
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  );
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return matrix[str2.length][str1.length];
Â  Â  Â  Â  }




Â  Â  Â  Â  function parseDuration(duration) {
Â  Â  Â  Â  Â  Â  if (!duration) return null;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // If already in milliseconds
Â  Â  Â  Â  Â  Â  if (/^\d+$/.test(duration)) {
Â  Â  Â  Â  Â  Â  Â  Â  return parseInt(duration);
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // If in MM:SS format
Â  Â  Â  Â  Â  Â  const timeMatch = duration.match(/^(\d+):(\d+)$/);
Â  Â  Â  Â  Â  Â  if (timeMatch) {
Â  Â  Â  Â  Â  Â  Â  Â  const minutes = parseInt(timeMatch[1]);
Â  Â  Â  Â  Â  Â  Â  Â  const seconds = parseInt(timeMatch[2]);
Â  Â  Â  Â  Â  Â  Â  Â  return (minutes * 60 + seconds) * 1000;
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  return null;
Â  Â  Â  Â  }




Â  Â  Â  Â  // Keyboard shortcuts
Â  Â  Â  Â  document.addEventListener('keydown', (event) => {
Â  Â  Â  Â  Â  Â  // Ctrl/Cmd + Enter to search
Â  Â  Â  Â  Â  Â  if ((event.ctrlKey || event.metaKey) && event.key === 'Enter') {
Â  Â  Â  Â  Â  Â  Â  Â  event.preventDefault();
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('search-btn').click();
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  // Escape to clear results
Â  Â  Â  Â  Â  Â  if (event.key === 'Escape') {
Â  Â  Â  Â  Â  Â  Â  Â  const resultsSection = document.getElementById('results-section');
Â  Â  Â  Â  Â  Â  Â  Â  if (!resultsSection.classList.contains('hidden')) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  resultsSection.classList.add('hidden');
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  mappingResults = [];
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  });




Â  Â  Â  Â  // Auto-save functionality (using sessionStorage alternative)
Â  Â  Â  Â  function saveToCache(key, data) {
Â  Â  Â  Â  Â  Â  try {
Â  Â  Â  Â  Â  Â  Â  Â  // Since localStorage is not available, use a simple in-memory cache
Â  Â  Â  Â  Â  Â  Â  Â  if (!window.appCache) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  window.appCache = {};
Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  Â  Â  window.appCache[key] = data;
Â  Â  Â  Â  Â  Â  } catch (error) {
Â  Â  Â  Â  Â  Â  Â  Â  console.log('Cache not available:', error);
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }




Â  Â  Â  Â  function loadFromCache(key) {
Â  Â  Â  Â  Â  Â  try {
Â  Â  Â  Â  Â  Â  Â  Â  return window.appCache ? window.appCache[key] : null;
Â  Â  Â  Â  Â  Â  } catch (error) {
Â  Â  Â  Â  Â  Â  Â  Â  console.log('Cache not available:', error);
Â  Â  Â  Â  Â  Â  Â  Â  return null;
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }




Â  Â  Â  Â  // Auto-save form data
Â  Â  Â  Â  function autoSaveFormData() {
Â  Â  Â  Â  Â  Â  const formData = {
Â  Â  Â  Â  Â  Â  Â  Â  trackName: document.getElementById('track-name').value,
Â  Â  Â  Â  Â  Â  Â  Â  artistName: document.getElementById('artist-name').value,
Â  Â  Â  Â  Â  Â  Â  Â  albumName: document.getElementById('album-name').value,
Â  Â  Â  Â  Â  Â  Â  Â  duration: document.getElementById('duration').value,
Â  Â  Â  Â  Â  Â  Â  Â  playlistUrl: document.getElementById('playlist-url').value,
Â  Â  Â  Â  Â  Â  Â  Â  batchTracks: document.getElementById('batch-tracks').value
Â  Â  Â  Â  Â  Â  };
Â  Â  Â  Â  Â  Â  saveToCache('formData', formData);
Â  Â  Â  Â  }




Â  Â  Â  Â  // Load saved form data
Â  Â  Â  Â  function loadSavedFormData() {
Â  Â  Â  Â  Â  Â  const savedData = loadFromCache('formData');
Â  Â  Â  Â  Â  Â  if (savedData) {
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('track-name').value = savedData.trackName || '';
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('artist-name').value = savedData.artistName || '';
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('album-name').value = savedData.albumName || '';
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('duration').value = savedData.duration || '';
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('playlist-url').value = savedData.playlistUrl || '';
Â  Â  Â  Â  Â  Â  Â  Â  document.getElementById('batch-tracks').value = savedData.batchTracks || '';
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  }




Â  Â  Â  Â  // Add event listeners for auto-save
Â  Â  Â  Â  ['track-name', 'artist-name', 'album-name', 'duration', 'playlist-url', 'batch-tracks'].forEach(id => {
Â  Â  Â  Â  Â  Â  const element = document.getElementById(id);
Â  Â  Â  Â  Â  Â  if (element) {
Â  Â  Â  Â  Â  Â  Â  Â  element.addEventListener('input', autoSaveFormData);
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  });




Â  Â  Â  Â  // Load saved data on page load
Â  Â  Â  Â  window.addEventListener('load', loadSavedFormData);




