Â  Â  Â  Â  document.getElementById('export-csv').addEventListener('click', () => {
Â  Â  Â  Â  Â  Â  if (!mappingResults || Object.keys(mappingResults).length === 0) {
Â  Â  Â  Â  Â  Â  Â  Â  showError("No results to export. Please run a search first.");
Â  Â  Â  Â  Â  Â  Â  Â  return;
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  let csv = 'Spotify Track,Spotify Artist,Spotify Album,Apple Music Track,Apple Music Artist,Apple Music Album,Confidence,Apple Music ID\n';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  if (mappingResults.matches) {
Â  Â  Â  Â  Â  Â  Â  Â  // Single track
Â  Â  Â  Â  Â  Â  Â  Â  mappingResults.matches.forEach(match => {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  const query = mappingResults.query;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  csv += `"${query.name}","${query.artist}","${query.album}","${match.name}","${match.artist}","${match.album}",${match.confidence},"${match.appleId || ''}"\n`;
Â  Â  Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  Â  Â  } else if (mappingResults.results) {
Â  Â  Â  Â  Â  Â  Â  Â  // Multiple tracks
Â  Â  Â  Â  Â  Â  Â  Â  mappingResults.results.forEach(result => {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  const spotify = result.spotify || result.parsed;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  const apple = result.appleMusic;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  if (apple) {
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  csv += `"${spotify.name}","${spotify.artist}","${spotify.album || ''}","${apple.name}","${apple.artist}","${apple.album || ''}",${apple.confidence},"${apple.appleId || ''}"\n`;
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  Â  Â  });
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const blob = new Blob([csv], { type: 'text/csv' });
Â  Â  Â  Â  Â  Â  const url = URL.createObjectURL(blob);
Â  Â  Â  Â  Â  Â  const a = document.createElement('a');
Â  Â  Â  Â  Â  Â  a.href = url;
Â  Â  Â  Â  Â  Â  a.download = 'spotify-apple-music-mapping.csv';
Â  Â  Â  Â  Â  Â  a.click();
